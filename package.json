{
  "name": "algo-zoo",
  "version": "0.0.0",
  "description": "Algo-zoo is a webpage for algorithm and data structure visualizers",
  "repository": "https://github.com/algo-zoo/algo-zoo.github.io",
  "author": "Yosuke Fukuda",
  "license": "MIT",
  "scripts": {
    "start": "hugo server --disableFastRender",
    "build": "NODE_ENV=production sh build.sh"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.4.16",
    "parcel": "^2.13.3",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0 ",
    "postcss-import": "^16.0.0",
    "tailwindcss": "^3.4.1"
  },
  "dependencies": {
    "rescript": "^11.1.4",
    "@rescript/core": "^1.6.1"
  },
  "browserslist": [
    "last 1 version",
    "> 1%",
    "maintained node versions",
    "not dead"
  ],
  "targets": {
    "es": {
      "context": "browser",
      "outputFormat": "esmodule",
      "distDir": "./static/js"
    }
  },
  "alias": {
    "rescript/lib/es6": "./node_modules/rescript/lib/es6"
  }
}
